  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.3.2",
    "rollup": "^3.25.3",
    "tslib": "^2.6.0"
  },
  "peerDependencies": {
    "@formkit/core": "^0.17.3",
